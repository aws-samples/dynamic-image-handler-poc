{
  "name": "dyamic-image-handler",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "types": "./types/global.d.ts",
  "scripts": {
    "start": "nodemon index.js",
    "build": "npm run lint && tsc -p . && cp .env ./build/.env",
    "build:test": "npm run lint && tsc -p . && cp .env ./build/.env && cp .env.test ./build/.env.test",
    "build:prod": "npm run lint && tsc -p . && cp .env ./build/.env && cp .env.prod ./build/.env.prod",
    "build:stag": "npm run lint && tsc -p . && cp .env ./build/.env && cp .env.stag ./build/.env.stag",
    "start:local": "npm run lint && NODE_ENV=local nodemon index.js",
    "start:test": "NODE_ENV=test node build/src/server.js",
    "start:staging": "NODE_ENV=staging node build/src/server.js",
    "start:production": "NODE_ENV=production node build/src/server.js",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "prettier-format": "npx prettier --config .prettierrc.yml '**/*.ts' --write",
    "prepare": "husky install",
    "test:custom": "npm test --  tests/unit-tests/environments/environment.spec.ts",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint ",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Madhu Balaji",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "@types/winston": "^2.4.4",
    "aws-sdk": "^2.1361.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "sharp": "^0.32.6",
    "typescript": "^4.9.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.21",
    "prettier": "2.8.8",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1"
  }
}
